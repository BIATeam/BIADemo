{
  "DatabaseMigrate": {
    "CommandTimeout": 1 //in minutes
  },
  "Tasks": {
    "WakeUp": {
      "CRON": "0 6-17 * * *"
    },
    // Begin BIADemo
    "Archive": {
      "CRON": "0 3 * * *"
    },
    // End BIADemo
    "SynchronizeUser": {
      "CRON": "0 6 * * *"
    }
  },
  "Project": {
    "Name": "BIADemo",
    "ShortName": "BIADemo"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "NLog": {
    "targets": {
      "fileLog": {
        "type": "File",
        "fileName": "E:\\Logs\\BIADemo\\${shortdate}_TheBIADevCompany.BIADemo_DeployDB.log",
        "archiveAboveSize": 2000000,
        "archiveNumbering": "Rolling",
        "archiveEvery": "Day",
        "layout": "${longdate}|${uppercase:${level}}|${aspnet-traceidentifier}|${message} ${exception:format=tostring}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minlevel": "Info",
        "writeTo": "fileLog"
      }
    ]
  }
}