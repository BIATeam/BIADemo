<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>5.2.0</Version>
		<Description>Common classes for BIA.Net Core Framework</Description>
		<Copyright>Copyright © BIA 2025</Copyright>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<Authors>BIA Team</Authors>
		<Company>BIA</Company>
		<PackageIcon>LogoBIA.png</PackageIcon>
		<PackageIconUrl />
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\Images\LogoBIA.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="[10.0.*,11.0.0)" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="[10.0.*,11.0.0)" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="[10.0.*,11.0.0)" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.*" />
	</ItemGroup>


	<Target Name="PreBuild" BeforeTargets="PreBuildEvent">
	</Target>

</Project>
