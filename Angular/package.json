{
  "name": "biademo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start4iis": "ng serve --configuration iis",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "clean": "npx prettier --write . && ng lint --fix",
    "build:iis": "ng build --configuration iis",
    "build:aot": "ng build --aot --base-href /BIADemo/App/ --configuration iis",
    "build:aot2": "ng build --aot --base-href /BIADemo2/App/ --output-path dist/BIADemo2 --configuration iis",
    "build:prod": "ng build --configuration production --base-href /BIADemo/App/",
    "build:prod2": "ng build --configuration production --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "build:ocp": "ng build --configuration production,ocp",
    "build:docker": "ng build --configuration docker",
    "deploy": "ng deploy --base-href=/BIADemo/",
    "styles-theme-dark": "dart compile-sass.dart ./src/assets/bia/primeng/bia/theme-dark.scss ./src/assets/bia/primeng/layout/styles/theme/theme-dark/theme-dark.css",
    "styles-theme-light": "dart compile-sass.dart ./src/assets/bia/primeng/bia/theme-light.scss ./src/assets/bia/primeng/layout/styles/theme/theme-light/theme-light.css",
    "styles-layout": "dart compile-sass.dart ./src/assets/bia/primeng/bia/layout-primeng-bia.scss ./src/assets/bia/primeng/layout/styles/layout/layout.css --output-style compressed",
    "styles": "npm run styles-theme-dark && npm run styles-theme-light && npm run styles-layout",
    "stats-compute": "ng build --configuration production --base-href /BIADemo/App/ --stats-json",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIADemo/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.12",
    "@angular/cdk": "16.2.14",
    "@angular/common": "17.3.12",
    "@angular/compiler": "17.3.12",
    "@angular/core": "17.3.12",
    "@angular/forms": "17.3.12",
    "@angular/platform-browser": "17.3.12",
    "@angular/platform-browser-dynamic": "17.3.12",
    "@angular/router": "17.3.12",
    "@angular/service-worker": "17.3.12",
    "@microsoft/signalr": "6.0.25",
    "@ngrx/effects": "17.2.0",
    "@ngrx/entity": "17.2.0",
    "@ngrx/store": "17.2.0",
    "@ngx-translate/core": "15.0.0",
    "@types/file-saver": "2.0.7",
    "date-fns": "3.6.0",
    "deepmerge": "4.3.1",
    "dexie": "3.2.7",
    "file-saver": "2.0.5",
    "jwt-decode": "4.0.0",
    "keycloak-angular": "15.3.0",
    "keycloak-js": "21.1.2",
    "ngx-logger": "5.0.12",
    "papaparse": "5.4.1",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primeng": "17.18.10",
    "rxjs": "7.8.1",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.9",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "17.3.9",
    "@angular/compiler-cli": "17.3.12",
    "@angular/language-service": "17.3.12",
    "@ngrx/store-devtools": "17.2.0",
    "@types/node": "20.16.5",
    "@types/papaparse": "5.3.14",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "prettier-eslint": "16.3.0",
    "ts-node": "10.9.2",
    "tslib": "2.7.0",
    "typescript": "5.4.5"
  }
}
