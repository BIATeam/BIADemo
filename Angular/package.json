{
  "name": "biademo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start4iis": "ng serve --configuration iis",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:iis": "ng build --configuration iis",
    "build:aot": "ng build --aot --base-href /BIADemo/App/ --configuration iis",
    "build:aot2": "ng build --aot --base-href /BIADemo2/App/ --output-path dist/BIADemo2 --configuration iis",
    "build:prod": "ng build --configuration production --base-href /BIADemo/App/",
    "build:prod2": "ng build --configuration production --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "build:ocp": "ng build --configuration production,ocp",
    "build:docker": "ng build --configuration docker",
    "deploy": "ng deploy --base-href=/BIADemo/",
    "styles-theme-dark": "node-sass ./src/assets/bia/primeng/theme/biaTheme/theme-dark.scss ./src/assets/bia/primeng/theme/biaTheme/theme-dark.css --output-style compressed",
    "styles-theme-light": "node-sass ./src/assets/bia/primeng/theme/biaTheme/theme-light.scss ./src/assets/bia/primeng/theme/biaTheme/theme-light.css --output-style compressed",
    "styles-layout-dark": "node-sass ./src/assets/bia/primeng/layout/css/layout-dark.scss ./src/assets/bia/primeng/layout/css/layout-dark.css --output-style compressed",
    "styles-layout-light": "node-sass ./src/assets/bia/primeng/layout/css/layout-light.scss ./src/assets/bia/primeng/layout/css/layout-light.css --output-style compressed",
    "styles": "npm run styles-theme-dark && npm run styles-theme-light && npm run styles-layout-dark && npm run styles-layout-light",
    "stats-compute": "ng build --configuration production --base-href /ESuitePortal/AppPortal/ --statsJson=true",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIADemo/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@microsoft/signalr": "^6.0.8",
    "@ngrx/effects": "^14.3.3",
    "@ngrx/entity": "^14.3.3",
    "@ngrx/store": "^14.3.3",
    "@ngx-translate/core": "^14.0.0",
    "@types/file-saver": "^2.0.5",
    "deepmerge": "^4.2.2",
    "dexie": "^3.2.2",
    "file-saver": "^2.0.5",
    "keycloak-angular": "^12.2.0",
    "keycloak-js": "^19.0.3",
    "ngx-logger": "^5.0.11",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^14.2.3",
    "rxjs": "~6.6.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@ngrx/store-devtools": "^14.3.3",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "angular-cli-ghpages": "^1.0.3",
    "eslint": "^8.23.0",
    "prettier": "1.18.1",
    "ts-node": "~7.0.0",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4"
  }
}
