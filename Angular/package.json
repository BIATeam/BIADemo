{
  "name": "biademo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start4iis": "ng serve --configuration iis",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:iis": "ng build --configuration iis",
    "build:aot": "ng build --aot --base-href /BIADemo/App/ --configuration iis",
    "build:aot2": "ng build --aot --base-href /BIADemo2/App/ --output-path dist/BIADemo2 --configuration iis",
    "build:prod": "ng build --configuration production --base-href /BIADemo/App/",
    "build:prod2": "ng build --configuration production --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "build:ocp": "ng build --configuration production,ocp",
    "build:docker": "ng build --configuration docker",
    "deploy": "ng deploy --base-href=/BIADemo/",
    "styles-theme-dark": "dart compile-sass.dart ./src/assets/bia/primeng/bia/theme-dark.scss ./src/assets/bia/primeng/layout/styles/theme/theme-dark/theme-dark.css",
    "styles-theme-light": "dart compile-sass.dart ./src/assets/bia/primeng/bia/theme-light.scss ./src/assets/bia/primeng/layout/styles/theme/theme-light/theme-light.css",
    "styles-layout": "dart compile-sass.dart ./src/assets/bia/primeng/bia/layout-primeng-bia.scss ./src/assets/bia/primeng/layout/styles/layout/layout.css --output-style compressed",
    "styles": "npm run styles-theme-dark && npm run styles-theme-light && npm run styles-layout",
    "stats-compute": "ng build --configuration production --base-href /ESuitePortal/AppPortal/ --statsJson=true",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIADemo/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/cdk": "^16.2.8",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/forms": "^16.2.9",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@angular/service-worker": "^16.2.9",
    "@microsoft/signalr": "^6.0.23",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/entity": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@types/file-saver": "^2.0.5",
    "deepmerge": "^4.3.1",
    "dexie": "^3.2.4",
    "file-saver": "^2.0.5",
    "keycloak-angular": "^14.1.0",
    "keycloak-js": "^21.1.2",
    "ngx-logger": "^5.0.12",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.5.0",
    "rxjs": "~7.8.1",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^16.2.9",
    "@angular/language-service": "^16.2.9",
    "@ngrx/store-devtools": "^16.3.0",
    "@types/node": "^16.18.39",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "angular-cli-ghpages": "^1.0.3",
    "eslint": "^8.39.0",
    "prettier": "1.18.1",
    "ts-node": "~7.0.0",
    "tslib": "^2.4.0",
    "typescript": "~5.1.6"
  }
}