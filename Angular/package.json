{
  "name": "biademo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start4iis": "ng serve --configuration iis",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "clean": "npx prettier --write . --plugin=prettier-plugin-organize-imports && ng lint --fix",
    "build:iis": "ng build --configuration iis",
    "build:aot": "ng build --aot --base-href /BIADemo/App/ --configuration iis",
    "build:aot2": "ng build --aot --base-href /BIADemo2/App/ --output-path dist/BIADemo2 --configuration iis",
    "build:prod": "ng build --configuration production --base-href /BIADemo/App/",
    "build:prod2": "ng build --configuration production --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "build:ocp": "ng build --configuration production,ocp",
    "build:rosa": "ng build --configuration production,rosa",
    "build:docker": "ng build --configuration docker",
    "build-bia-ng": "ng build bia-ng",
    "build-bia-ng:prod": "ng build bia-ng --configuration production",
    "deploy": "ng deploy --base-href=/BIADemo/",
    "styles": "dart compile-sass.dart ./src/assets/bia/primeng/bia/layout-primeng-bia.scss ./src/assets/bia/primeng/layout/styles/layout/layout.css --output-style compressed",
    "stats-compute": "ng build --configuration production --base-href /BIADemo/App/ --stats-json",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIADemo/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.3.16",
    "@angular/cdk": "~20.2.14",
    "@angular/common": "~20.3.16",
    "@angular/compiler": "~20.3.16",
    "@angular/core": "~20.3.16",
    "@angular/forms": "~20.3.16",
    "@angular/platform-browser": "~20.3.16",
    "@angular/platform-browser-dynamic": "~20.3.16",
    "@angular/router": "~20.3.16",
    "@angular/service-worker": "~20.3.16",
    "@bia-team/bia-ng": "@bia-team/bia-ng@6.0.2",
    "@microsoft/signalr": "~9.0.6",
    "@ngrx/effects": "~20.1.0",
    "@ngrx/entity": "~20.1.0",
    "@ngrx/operators": "~20.1.0",
    "@ngrx/store": "~20.1.0",
    "@ngx-translate/core": "~17.0.0",
    "@primeng/themes": "~20.5.0-lts",
    "@types/file-saver": "~2.0.7",
    "date-fns": "~4.1.0",
    "deepmerge": "~4.3.1",
    "dexie": "~4.3.0",
    "file-saver": "~2.0.5",
    "jwt-decode": "~4.0.0",
    "keycloak-angular": "~20.1.0",
    "keycloak-js": "~26.2.1",
    "ngx-logger": "~5.0.12",
    "papaparse": "~5.5.2",
    "primeflex": "~4.0.0",
    "primeicons": "~7.0.0",
    "primeng": "~20.5.0-lts",
    "quill": "~2.0.3",
    "rxjs": "~7.8.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.3.14",
    "@angular-devkit/core": "~20.3.14",
    "@angular-devkit/schematics": "~20.3.14",
    "@angular-eslint/builder": "~20.7.0",
    "@angular-eslint/eslint-plugin": "~20.7.0",
    "@angular-eslint/eslint-plugin-template": "~20.7.0",
    "@angular-eslint/schematics": "~20.7.0",
    "@angular-eslint/template-parser": "~20.7.0",
    "@angular/build": "~20.3.14",
    "@angular/cli": "~20.3.14",
    "@angular/compiler-cli": "~20.3.16",
    "@angular/language-service": "~20.3.16",
    "@ngrx/store-devtools": "~20.1.0",
    "@types/node": "~24.10.8",
    "@types/papaparse": "~5.5.2",
    "@typescript-eslint/eslint-plugin": "~8.54.0",
    "@typescript-eslint/parser": "~8.54.0",
    "eslint": "~9.39.1",
    "eslint-config-prettier": "~10.1.5",
    "eslint-plugin-prettier": "~5.5.5",
    "ng-packagr": "~20.3.2",
    "prettier": "~3.8.1",
    "prettier-eslint": "~16.4.2",
    "prettier-plugin-organize-imports": "~4.3.0",
    "ts-node": "~10.9.2",
    "tslib": "~2.8.1",
    "typescript": "~5.9.3"
  }
}