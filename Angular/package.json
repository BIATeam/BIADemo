{
  "name": "biademo",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:aot": "ng build --aot --base-href /BIADemo/App/",
    "build:aot2": "ng build --aot --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "build:prod": "ng build --configuration production --base-href /BIADemo/App/",
    "build:prod2": "ng build --configuration production --base-href /BIADemo2/App/ --output-path dist/BIADemo2",
    "deploy": "ng deploy --base-href=/BIADemo/",
    "styles-theme-dark": "node-sass ./src/assets/bia/primeng/theme/biaTheme/theme-dark.scss ./src/assets/bia/primeng/theme/biaTheme/theme-dark.css --output-style compressed",
    "styles-theme-light": "node-sass ./src/assets/bia/primeng/theme/biaTheme/theme-light.scss ./src/assets/bia/primeng/theme/biaTheme/theme-light.css --output-style compressed",
    "styles-layout-dark": "node-sass ./src/assets/bia/primeng/layout/css/layout-dark.scss ./src/assets/bia/primeng/layout/css/layout-dark.css --output-style compressed",
    "styles-layout-light": "node-sass ./src/assets/bia/primeng/layout/css/layout-light.scss ./src/assets/bia/primeng/layout/css/layout-light.css --output-style compressed",
    "styles": "npm run styles-theme-dark && npm run styles-theme-light && npm run styles-layout-dark && npm run styles-layout-light",
    "stats-compute": "ng build --configuration production --base-href /ESuitePortal/AppPortal/ --statsJson=true",
    "stats-display": "webpack-bundle-analyzer --port 4299 dist/BIADemo/stats.json",
    "stats": "npm run stats-compute && npm run stats-display"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@angular/service-worker": "~12.2.8",
    "@aspnet/signalr": "^1.1.4",
    "@fullcalendar/core": "^4.4.2",
    "@fullcalendar/daygrid": "^4.4.2",
    "@fullcalendar/interaction": "^4.4.2",
    "@fullcalendar/timegrid": "^4.4.2",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@ngx-translate/core": "^11.0.0",
    "@types/file-saver": "^2.0.1",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "chart.js": "^2.9.4",
    "deepmerge": "^4.2.2",
    "dexie": "^3.2.0",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.0",
    "intl": "^1.2.5",
    "keycloak-angular": "^9.1.0",
    "keycloak-js": "^18.0.0",
    "ngrx-store-logger": "^0.2.4",
    "ngx-logger": "^4.3.3",
    "primeflex": "^3.0.1",
    "primeicons": "^4.1.0",
    "primeng": "^12.2.1",
    "quill": "^1.3.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "tslint-angular": "^3.0.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "^12.2.8",
    "@angular/compiler-cli": "~12.2.8",
    "@angular/language-service": "~12.2.8",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "prettier": "1.18.1",
    "ts-node": "~7.0.0",
    "typescript": "~4.3.5",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
